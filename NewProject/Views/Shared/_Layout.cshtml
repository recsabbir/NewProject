                                    <button class="btn btn-primary" id="buttonUpdateSalaryInfo" onclick="UpdateSalaryInfo()">Update</button>

                                </div>
                            </div>
                        </div>
                    </div>
                    <!---------Salary Info Update Modal Ends------------>
