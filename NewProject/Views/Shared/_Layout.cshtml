                        <div class="modal-dialog modal-lg" role="document">

                            <div class="modal-content">
                                <div class="modal-header flex-column">
                                    <div>
                                        <i class="fas fa-plus" aria-hidden="true"></i>&nbsp; Distribute Item
                                    </div>
                                </div>
                                <div class="modal-body" id="AddItemDistributionBody">



